<%
  cacert = scope['::splunk::params::cacert']
  servercertpass = scope['splunk::params::servercertpass']
  servercert = scope['::splunk::params::servercert']
  ciphersuite = scope['::splunk::params::ciphersuite']
  sslversions = scope['::splunk::params::sslversions']
  sslclientcert = scope['::splunk::params::sslclientcert']
  splunknotcp = scope['::splunk::params::splunknotcp']
  splunknotcp_ssl = scope['::splunk::params::splunknotcp_ssl']
-%>

[SSL]
password = <%= servercertpass %>
cipherSuite = <%= ciphersuite %>
requireClientCert = <%= sslclientcert %>
rootCA = $SPLUNK_HOME/etc/auth/<%= cacert %>
serverCert = $SPLUNK_HOME/etc/auth/<%= servercert %>
sslVersions = <%= sslversions %>

[splunktcp://9997]
disabled = <%= splunknotcp %>

[splunktcp-ssl://9998]
disabled = <%= splunknotcp_ssl %>
